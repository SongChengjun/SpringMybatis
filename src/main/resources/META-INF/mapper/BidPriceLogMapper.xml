<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.carsmart.song.mapper.BidPriceLogMapper">

    <insert id="insert" >
        INSERT INTO bidPriceLog (id,lot_id,bidder_id,price,bidTime)
          VALUES (#{id},#{lotId},#{bidderId},#{price},#{bidTime})
    </insert>

</mapper>